  "files": [
    "*.js"
  ],
  "dependencies": {
    "bipf": "^1.9.0",
    "clarify-error": "^1.0.0",
    "pull-level": "^2.0.4",
    "pull-stream": "^3.6.14"
  },
  "peerDependencies": {
    "ssb-db2": "^4.1.3"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/pull-stream": "^3.6.2",
    "husky": "^7.0.4",
    "level": "^8.0.0",
    "p-defer": "^4.0.0",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "secret-stack": "^6.4.1",
    "ssb-caps": "^1.1.0",
    "ssb-db2": "^4.1.3",
    "ssb-fixtures": "^3.0.4",
    "ssb-keys": "^8.4.0",
    "tap-arc": "^0.3.4",
    "tape": "^5.5.3",
    "typescript": "~4.7.4"
